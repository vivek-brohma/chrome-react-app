{
	"name": "Repaint",
	"description": "Bare bone image editor",
	"version": "1.0.0.0",
	"short_name": "Repaint",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"128": "images/logo.png"
	},
	"permissions": [
		"storage",
		{"fileSystem": ["retainEntries"]}
	]
}